<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taikang.health.iams.dao.AutzUserClientDao">

    <resultMap id="AutzUserClientResultMap" type="com.taikang.health.iams.po.AutzUserClientPO">
        <id property="id"               column="user_role_id" javaType="String" jdbcType="VARCHAR"/>
        <result property="userId"       column="user_id"      javaType="String" jdbcType="VARCHAR"/>
        <result property="clientId"     column="role_id"      javaType="String" jdbcType="VARCHAR"/>
        <result property="createBy"     column="create_by"    javaType="String" jdbcType="VARCHAR"/>
        <result property="createTime"   column="create_time"  javaType="Date"   jdbcType="DATE"/>
        <result property="updateBy"   column="update_by"  javaType="String" jdbcType="VARCHAR"/>
        <result property="updateTime"   column="update_time"  javaType="Date"   jdbcType="DATE"/>
    </resultMap>

    <!---========================自定义查询放置下方========================-->
    <insert id="insertBatch">
        insert into autz_user_client (user_client_id,user_id,client_id,create_by,create_time,update_by,update_time)
        values
        <foreach collection="list" item="e" separator=",">
            (#{e.id},#{e.userId},#{e.clientId},#{e.createBy},#{e.createTime},#{e.updateBy},#{e.updateTime})
        </foreach>
    </insert>
</mapper>